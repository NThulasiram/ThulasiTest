
@{
    ViewBag.Title = "Report";
}


<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>FGMC</title>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/dataTables.bootstrap.min.css" rel="stylesheet"/>
    @*<link href="~/Content/style3.css" rel="stylesheet"/>*@
    <link href="~/Content/CommonStyleSheet.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet"/>
    <script src="~/Scripts/html5.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.bootstrap.min.js"></script>
    @*  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>*@


    <script src="https://cdn.datatables.net/buttons/1.1.0/js/dataTables.buttons.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.1.0/css/buttons.dataTables.min.css"/>
    <script src="//cdn.datatables.net/buttons/1.1.0/js/buttons.flash.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jszip/2.5.0/jszip.min.js"></script>
    <script src="//cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/pdfmake.min.js"></script>
    <script src="//cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/vfs_fonts.js"></script>
    <script src="//cdn.datatables.net/buttons/1.1.0/js/buttons.html5.min.js"></script>
    <script src="//cdn.datatables.net/buttons/1.1.0/js/buttons.print.min.js"></script>


</head>

<div class="newMenuBar" id="newMenuBar">
    <ul class="home-nav text-left">


        @if (Session["CurrentlyLoggedInUser"] != null)
        {

            <li>@Html.ActionLink("Search Loans", "SearchLoans", "SearchLoans") </li>

        }

        @if (Session["IsAdmin"] != null)
        {
            if ((bool) Session["IsAdmin"])
             {

                 <li>@Html.ActionLink("Manage Users", "UserDetails", "ManageUser") </li>

             }
        }
         @if (Session["IsAdmin"] != null)
         {
             if ((bool) Session["IsAdmin"])
             {
                 <li>@Html.ActionLink("Manage Configurations", "Configurator", "ApplicationConfiguration") </li>
             }
         }

        

    </ul>

</div>




<body>

<section class="container-fluid">
    <div class="contant-wrap">
        <h3>Documents Extraction Reports</h3>
        <div class="alert alert-success" id="Sucessinfo">
            <h5><span></span></h5>
        </div>
        <div class="alert alert-danger" id="ErrorInfo">
            <h5><span></span></h5>
        </div>
        <div class="row wrap">
            <form class="form-inline wrap borderBtm" role="form">
                <div class="row">
                    <div class="col-lg-5">
                        <div class="form-group">
                            <label for="email">Date Range From: </label>
                            <input type="text" class="form-control date" id="datetimepicker1" onfocus="validate()" onfocusout="validate()">
                            <label for="email">To: </label>
                            <input type="text" class="form-control date" id="datetimepicker2" onfocus="validate()" onfocusout="validate()" onblur="dateRangeValidation()">
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="form-group">
                            <label for="Status">Document Extraction Status: </label>
                            @*<select class="form-control" id="Status" Name="Status" onchange="validate()" onfocusout="validate()">
                                <option value="-Select Value-">-Select Value-</option>
                                <option value="1">Submit</option>
                                <option value="2">Complete</option>
                                <option value="3">Fail</option>
                            </select>*@

                            @Html.DropDownList("DocumentExtractStatusName", (IEnumerable<SelectListItem>) ViewBag.DocumentExtractStatus, "-Select Value-", new {@class = "form-control", id = "Status", name = "DocumentExtractStatusName", @onchange = "validate()", @onfocusout = "validate()"}) <br/>

                        </div>
                    </div>
                    <div class="col-lg-2 text-right">
                        <button type="button" class="btn btn-default" id="generateReport" name="generateReport">Generate Reports</button>
                    </div>
                </div>
            </form>
        </div>
        @*<div class="row wrap">
            <div class="col-lg-12 text-right">
                <button type="submit" class="btn btn-default">Export Reports</button>
            </div>
        </div>*@
        <div class="table-responsive wrap">
            <table class="table table-bordered table-hover table-striped" id="importPool">
                <thead>
                <tr>
                    <th width="25px">Batch Id</th>
                    <th>Loan Number</th>
                    <th>Document Name</th>
                    <th>User Name</th>
                    <th>Date & Time</th>
                    <th>Document Extraction Status</th>
                </tr>
                </thead>
            </table>
        </div>
    </div>
</section>


<script>
    $(document).ready(function() {
        $('#importPool').DataTable({
        
        });

    });
</script>


